<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minWidth="25px"
    android:minHeight="25px">
    <Button  
        android:text="Load Data"  
        android:layout_width="match_parent"  
        android:layout_height="wrap_content"  
        android:id="@+id/btnLoadData" />
    <LinearLayout  
        android:orientation="vertical"  
        android:minWidth="25px"  
        android:minHeight="25px"  
        android:layout_width="match_parent"  
        android:layout_height="wrap_content"  
        android:id="@+id/container" />
    <Button
        android:text="@string/favorite"
        android:layout_width="match_parent"
        android:layout_height="78.0dp"
        android:layout_marginTop="20dp"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:textStyle="bold"
        android:id="@+id/btn_favorite" />
    <Button
        android:text="@string/src_by_num"
        android:layout_width="match_parent"
        android:layout_height="78.0dp"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:textStyle="bold"
        android:id="@+id/btn_src_num" />
    <Button
        android:text="@string/src_by_station"
        android:layout_width="match_parent"
        android:layout_height="78.0dp"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:textStyle="bold"
        android:id="@+id/btn_src_station" />
    <Button
        android:text="@string/map"
        android:layout_width="match_parent"
        android:layout_height="78.0dp"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:textStyle="bold"
        android:id="@+id/btn_map" />
    <Button
        android:text="@string/train"
        android:layout_width="match_parent"
        android:layout_height="78.0dp"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:textStyle="bold"
        android:id="@+id/btn_train" />
    <!--<Button
        android:text="map test"
        android:layout_width="match_parent"
        android:layout_height="78.0dp"
        android:textSize="@dimen/abc_dialog_padding_material"
        android:textStyle="bold"
        android:id="@+id/btn_maptst" />-->
</LinearLayout>
